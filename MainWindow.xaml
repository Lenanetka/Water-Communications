<Window x:Class="WaterCommunications.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WaterCommunications"
        mc:Ignorable="d"
        Title="Water communications" Height="299" Width="525">
    <Grid>
        <Button x:Name="bStart" Content="Start" Margin="209,226,233,0" VerticalAlignment="Top" Click="bStart_Click"/>
        <Label x:Name="labelH" Content="Enter H" HorizontalAlignment="Left" Margin="10,122,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.571,-1.154"/>
        <Label x:Name="LabelHMin" Content="Enter Hmin" HorizontalAlignment="Left" Margin="10,153,0,0" VerticalAlignment="Top"/>
        <Label x:Name="LabelAccidentPercent" Content="Enter accident percent of Q" HorizontalAlignment="Left" Margin="10,184,0,0" VerticalAlignment="Top"/>
        <Menu HorizontalAlignment="Left" Height="23" VerticalAlignment="Top" Width="517"/>
        <TextBox x:Name="tbH" HorizontalAlignment="Left" Height="23" Margin="194,122,0,0" TextWrapping="Wrap" Text="40" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="tbHMin" HorizontalAlignment="Left" Height="23" Margin="194,153,0,0" TextWrapping="Wrap" Text="30" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.525,-0.043"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="194,184,0,0" TextWrapping="Wrap" Text="0.7" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="labelSave" Content="Save in" HorizontalAlignment="Left" Margin="12,69,0,0" VerticalAlignment="Top"/>
        <Label x:Name="labelLoad" Content="Load from" HorizontalAlignment="Left" Margin="10,28,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tbLoadPath" HorizontalAlignment="Left" Height="23" Margin="136,28,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="291"/>
        <TextBox x:Name="tbSavePath" HorizontalAlignment="Left" Height="23" Margin="136,69,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="291"/>
        <Button x:Name="bBrowseLoadPath" Content="..." HorizontalAlignment="Left" Margin="432,28,0,0" VerticalAlignment="Top" Width="75" Click="bBrowseLoadPath_Click"/>
        <Button x:Name="bBrowseSavePath" Content="..." HorizontalAlignment="Left" Margin="432,69,0,0" VerticalAlignment="Top" Width="75" Click="bBrowseSavePath_Click"/>

    </Grid>
</Window>
